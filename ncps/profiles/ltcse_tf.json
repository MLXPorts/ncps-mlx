{
  "name": "tensorflow_legacy",
  "description": "Hyperparameter ranges and constraints matching experiments_with_ltcs/ltc_model.py",
  "ode_unfolds": 6,
  "solver": "semi_implicit",
  "input_mapping": "affine",
  "initializers": {
    "w_min": 0.01,
    "w_max": 1.0,
    "cm_min": 0.5,
    "cm_max": 0.5,
    "gleak_min": 1.0,
    "gleak_max": 1.0,
    "mu_min": 0.3,
    "mu_max": 0.8,
    "sigma_min": 3.0,
    "sigma_max": 8.0,
    "vleak_min": -0.2,
    "vleak_max": 0.2
  },
  "constraints": {
    "w_min": 1e-5,
    "w_max": 1e3,
    "gleak_min": 1e-5,
    "gleak_max": 1e3,
    "cm_min": 1e-6,
    "cm_max": 1e3
  }
}
